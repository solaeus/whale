table = create_table (
	("text", "number", "bool"),
	(
		("a", 1, true),
		("b", 2, true),
		("a", 3, true)
	)
);

test_table = create_table (
	("text", "bool"),
	(
		("a", true),
		("b", true),
		("a", true)
	)
);

assert_equal(table:select("text", "bool"), test_table);

# Select from table

test_table = create_table (
	("text", "number", "bool"),
	(
		("a", 1, true),
		("a", 3, true)
	)
);

assert_equal(table:where('text == "a"'), test_table);
